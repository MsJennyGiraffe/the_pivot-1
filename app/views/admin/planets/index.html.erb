<div class="container">
  <h1> All Planets </h1>

  <table id="planets-table" class="table table-hover">
    <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @planets.each do |planet| %>
      <tr>
      <td><%= planet.name %></td>
      <td><%= link_to "Edit #{planet.name}", edit_admin_planet_path(planet.id) %></td>
      <td><%= %></td>
      </tr>
    <% end %>
  </tbody>
  </table>
</div>
