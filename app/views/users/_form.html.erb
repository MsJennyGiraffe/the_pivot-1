<div class="col-md-4 col-md-offset-4">
  <div class="well">
  <%= form_for(@user) do |f| %>
    <fieldset class="form-group">
        <h4><%= f.label :first_name %>
        <span class="form-field-first-name"><%= f.text_field :first_name, class: "form-control" %></span></h4>
        <h4><%= f.label :last_name %>
        <span class="form-field-last-name"><%= f.text_field :last_name, class: "form-control" %></span></h4>
        <h4><%= f.label :username %>
        <span class="form-field-username"><%= f.text_field :username, class: "form-control" %></span></h4>
        <h4><%= f.label :password %>
        <span class="form-field-password"><%= f.password_field :password, class: "form-control" %></span></h4>
        <h4><%= f.label :password_confirmation %>
        <span class="form-field-passwordconf"><%= f.password_field :password_confirmation, class: "form-control" %></span></h4>
        <h4><%= f.label :email %>
        <span class="form-field-email"><%= f.text_field :email, class: "form-control" %></span></h4>
        <h4><%= f.label :email_confirmation %>
        <span class="form-field-emailconf"><%= f.text_field :email_confirmation, class: "form-control" %></span></h4>
        <h4><%= f.label :phone_number %>
        <span class="form-field-phone-number"><%= f.text_field :phone_number, class: "form-control" %></span></h4>
      </fieldset>
      <div class="col-md-4 col-md-offset-3">
        <%= f.submit button, class: "btn btn-person"%>
      </div>
      <br><br><br>
  <% end %>
  </div>
</div>
